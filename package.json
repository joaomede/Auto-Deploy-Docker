{
  "name": "auto-deploy-docker",
  "version": "0.0.1",
  "description": "A Technology for automatic deployment using docker",
  "author": "Jo√£o Medeiros N. Jr - <symbol2studio@gmail.com> (https://symbol2.com.br/)",
  "homepage": "http://github.com/joaomede/AutoDeployDocker#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+http://github.com/joaomede/AutoDeployDocker.git"
  },
  "keywords": [
    "deployment",
    "api",
    "express",
    "typescript",
    "docker",
    "git",
    "webhook"
  ],
  "private": true,
  "scripts": {
    "build": "rimraf dist/ && sucrase ./api -d ./dist --transforms typescript,imports",
    "dev": "nodemon api/server.ts",
    "start:dev": "sucrase-node api/server.ts",
    "migrate": "sucrase-node api/db/migrate",
    "migrate:down": "sucrase-node api/db/migratedown",
    "build:view": "cd view && yarn quasar build -m pwa",
    "start": "node dist/server.js",
    "version": "git add -A ./",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "bcrypt": "^3.0.7",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "dockerode": "^3.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.8",
    "knex-populate": "^2.1.2",
    "lodash": "^4.17.15",
    "nodemailer": "^6.4.2",
    "pg": "^7.18.1",
    "resp-express": "^2.0.0",
    "sqlite3": "^4.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/cors": "^2.8.6",
    "@types/dockerode": "^2.5.21",
    "@types/express": "^4.17.2",
    "@types/hapi__joi": "^16.0.4",
    "@types/jest": "^25.1.2",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.0",
    "@types/nodemailer": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "sucrase": "^3.12.1",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5"
  }
